{
  "name": "@viamrobotics/three",
  "version": "0.0.4",
  "license": "Apache-2.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/ov.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/ov.js"
    }
  },
  "scripts": {
    "start": "vite --host --open --config vite.site.config.ts",
    "build": "vite build && tsc --emitDeclarationOnly",
    "build-site": "vite build --outDir site --config vite.site.config.ts",
    "check": "tsc --noEmit",
    "preview": "vite preview --open --host",
    "test": "vitest",
    "test-run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "three",
    "three.js",
    "3d",
    "orientation vector",
    "viam"
  ],
  "devDependencies": {
    "@0b5vr/tweakpane-plugin-rotation": "^0.2.0",
    "@tweakpane/core": "^2.0.1",
    "@tweakpane/plugin-essentials": "^0.2.0",
    "@types/three": "^0.159.0",
    "three": "^0.159.0",
    "three-inspect": "^0.3.4",
    "trzy": "^0.3.17",
    "tweakpane": "^4.0.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "three": "*"
  }
}
