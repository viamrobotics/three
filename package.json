{
  "name": "@viamrobotics/three",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/ov.umd.js",
  "module": "./dist/ov.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ov.js",
      "require": "./dist/ov.umd.js"
    }
  },
  "scripts": {
    "start": "vite --host --open --config vite.site.config.ts",
    "build": "vite build && tsc --emitDeclarationOnly",
    "build-site": "vite build --outDir site --config vite.site.config.ts",
    "check": "tsc --noEmit",
    "preview": "vite preview --open --host",
    "test": "vitest",
    "test-run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "three",
    "three.js",
    "3d",
    "orientation vector",
    "viam"
  ],
  "devDependencies": {
    "@0b5vr/tweakpane-plugin-rotation": "^0.1.1",
    "@tweakpane/core": "^1.1.9",
    "@tweakpane/plugin-essentials": "^0.1.8",
    "@types/three": "^0.154.0",
    "three-inspect": "^0.3.4",
    "trzy": "^0.2.0",
    "tweakpane": "^3.1.10",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "three": "*"
  }
}
